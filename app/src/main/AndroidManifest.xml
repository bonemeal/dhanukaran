<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dhanuka.customer.dhanuka">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".pendingOrders.PendingOrdersActivity" />
        <activity android:name=".flvisit.FlVisitActivity" />
        <activity android:name=".pastTransactions.PastTransactionsActivity" />
        <activity android:name=".convertionRate.ConvertionRateActivity" />
        <activity android:name=".brandAdditions.BrandAdditionsActivity" />
        <activity android:name=".pendingIssues.PendingIssuesActivity" />
        <activity android:name=".pendingOrders.PendingOrdersProductActivity" />
        <activity android:name=".customerOutstanding.CustomerOutstandingActivity" />
        <activity android:name=".paymentCommitment.PaymentCommitmentActivity" />
        <activity android:name=".paymentAdherence.PaymentAdherenceActivity" />
        <activity android:name=".incentives.IncentivesActivity"></activity>
    </application>

</manifest>